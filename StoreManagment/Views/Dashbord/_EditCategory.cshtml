@model StoreManagment.ViewModel.AddProductVM


<div class="modal fade" id="myModalEdit" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true" data-bs-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="exampleModalLabel">Update Caategory</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div class="form-group row">
                    <label asp-for="Product_Category" class="col-sm-2 col-form-label">Category</label>
                    <div class="col-sm-10">
                        <input asp-for="Product_Category" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-success">Update</button>
                <button type="button" class="btn btn-outline-danger text-black" data-bs-dismiss="modal">Close</button>
            </div>
        </div>
       
    </div>
</div>


