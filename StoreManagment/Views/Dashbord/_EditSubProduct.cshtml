@model StoreManagment.ViewModel.Sub_ProductVM


<div class="modal fade" id="myModalEdit" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true" data-bs-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="exampleModalLabel">Update Caategory</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
             
                <div class="form-group row">
                    @*Id For Update Sub Product*@
                    <input asp-for="Sub_P_Id" type="hidden" class="form-control" id="Sub_P_UpdateId" />
                    <label asp-for="ProductCategory" class="col-sm-2 col-form-label">Category</label>
                    <div class="col-sm-10">
                        <select asp-for="ProductCategory" asp-items="Model.categoryList" class="form-control" id="U_SubProductCategory">
                            <option> --Select Category--</option>
                        </select>
                    </div>
                </div>
                <br />
                <div class="form-group row">
                    <label asp-for="Sub_Product_Name" class="col-sm-2 col-form-label">Category</label>
                    <div class="col-sm-10">
                        <input asp-for="Sub_Product_Name" class="form-control" id="USub_Product" />
                    </div>
                </div>

              
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-success" id="BtnUpdate_SubProduct" data-id="Sub_P_Id">Update</button>
                <button type="button" class="btn btn-outline-danger text-black" data-bs-dismiss="modal">Close</button>
            </div>
        </div>

    </div>
</div>


